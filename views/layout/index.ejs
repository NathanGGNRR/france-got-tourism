<%- include('../includes/header'); %>
<body>
    <!-- banner part start-->
    <section class="index-header_bg align-items-center shadow-lg">
        <div class="container d-flex flex-column align-items-center">
            <img src="assets/img/index/logoFGT.png" >
            <h3 class="text-white font-weight-light site-title">France Got Tourism</h3>            
            <a class="waves-effect waves-light btn-large my-auto text-white" href="list-activities">Research Now</a>
